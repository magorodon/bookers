<body>

<h1>Books</h1>

<h2>New book</h2>

<%= form_with model: @book, local:true do |f| %>
  <div class="area">
    <p>本のタイトル</p>
    <%= f.text_field :title %>
  </div>
  <div class="area">
    <p>感想</p>
    <%= f.text_area :body %>
  </div>
  <div class="toukou">
    <%= f.submit 'Create Book' %>
  </div>
<% end %>


</body>
